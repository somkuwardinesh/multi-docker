{
  "AWSEBDockerrunVersion": "2",
  "containerDefinations": [
    	{
	    "name": "client",
	    "image": "dineshdocker3246/multi-client",
	    "hostname": "client",
	    "essential": false,
	    "memory": 128
   	},
   	{
	    "name": "server",
	    "image": "dineshdocker3246/multi-server",
	    "hostname": "api",
	    "essential": false,
	    "memory": 128
   	},
   	{
	    "name": "worker",
	    "image": "dineshdocker3246/multi-worker",
	    "hostname": "worker",
	    "essential": false,
	    "memory": 128
   	},
   	{
	    "name": "nginx",
	    "image": "dineshdocker3246/multi-nginx",
	    "hostname": "nginx",
	    "essential": false,
	    "portMappings":[
	    	{
	    	  "hostPort":80,
	    	  "containerPort":80
	    	}
	    ],
	    "links": ["client","server"],
	    "memory": 128
   	}	
  ]
}      
